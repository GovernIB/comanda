        <subsystem xmlns="urn:jboss:domain:keycloak:1.1">
            <realm name="GOIB">
                <auth-server-url>${env.JBOSS_AUTH_URL}</auth-server-url>
                <ssl-required>none</ssl-required>
            </realm>
            <secure-deployment name="comanda-back.war">
                <realm>${env.JBOSS_AUTH_REALM}</realm>
                <resource>${env.JBOSS_AUTH_CLIENTID_WS}</resource>
                <use-resource-role-mappings>true</use-resource-role-mappings>
                <bearer-only>true</bearer-only>
                <enable-basic-auth>true</enable-basic-auth>
                <credential name="secret">${env.JBOSS_AUTH_CREDENTIAL_SECRET}</credential>
                <principal-attribute>preferred_username</principal-attribute>
            </secure-deployment>
        </subsystem>
